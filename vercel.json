{
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "http://packman-app.vercel.app/$1"
      }
    ]
  }
  {
    "version": 2,
    "builds": [
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "https://packman-app.vercel.app/$1",
        "status": 301,
        "headers": {
          "Location": "http://packman-app.vercel.app/$1"
        }
      }
    ]
  }
  